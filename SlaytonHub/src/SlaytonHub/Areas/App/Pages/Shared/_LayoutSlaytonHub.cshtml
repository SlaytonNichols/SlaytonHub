<!DOCTYPE html>
<html>
<head>
    <partial name="_HeadPartial" />
    <link rel="stylesheet" href="~/admin-lte/css/AdminLTE.min.css">
    @RenderSection("styles", required: false)
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <partial name="_HeaderPartial" />

        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        <partial name="_SidebarPartial" />

        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">`

            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewData["Title"]
                    <small>@ViewData["SubTitle"]</small>
                </h1>
                @RenderSection("breadcrumb", required: false)

            </section>
            <!-- Main content -->
            <section class="content">
                <!-- Default box -->
                @RenderBody()

                <!-- /.box -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0
            </div>
            <strong>Copyright &copy; 2020 <a href="https://nicholsslayton.com">Slayton Nichols</a>.</strong> All rights
            reserved.
        </footer>
    </div>
    <!-- ./wrapper -->
    <partial name="_ScriptsPartial" />
    @RenderSection("Scripts", required: false)
</body>
</html>
